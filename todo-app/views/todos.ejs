<table>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Due Date</th>
        <th>Completed?</th>
    </tr>
    <% for (let todo of allTodos) { %>
        <tr>
            <td> <%= todo.id %></td>
            <td> <%= todo.title %> </td>
            <td> <%= todo.dueDate %> </td>
            <td> <%= todo.completed %> </td>
        </tr>
    <% } %>
</table>